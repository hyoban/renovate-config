{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:recommended", ":semanticCommits", ":semanticCommitTypeAll(chore)"],
  "packageRules": [
    {
      "depTypeList": ["peerDependencies"],
      "enabled": false
    }
  ],
  "labels": ["dependencies"],
  "rangeStrategy": "bump",
  "ignoreDeps": ["node", "@types/vscode", "vscode"]
}
